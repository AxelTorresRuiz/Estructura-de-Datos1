<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" spacing="20.0" styleClass="fondo" stylesheets="@estilos.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label text="VideoJuegos">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font size="41.0" />
         </font>
      </Label>
      <TextField fx:id="txtusuario" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Usuario" styleClass="textfield" />
      <PasswordField fx:id="txtpassword" maxHeight="30.0" maxWidth="200.0" onKeyReleased="#enter" promptText="Password" styleClass="textfield" />
      <Button maxHeight="30.0" maxWidth="150.0" mnemonicParsing="false" onAction="#login" styleClass="buttonAceptar" text="Ingresar" textFill="WHITE" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
