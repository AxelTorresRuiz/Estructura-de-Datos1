<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Vacuna.vacuna">
   <children>
      <Button fx:id="btninsert" layoutX="506.0" layoutY="30.0" mnemonicParsing="false" onAction="#insertar" text="INSERTAR" />
