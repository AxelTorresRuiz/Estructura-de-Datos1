      <TextField fx:id="txtname" layoutX="17.0" layoutY="55.0" prefHeight="25.0" prefWidth="121.0" promptText="NAME" />
      <TextField fx:id="txtmail" layoutX="286.0" layoutY="55.0" prefHeight="25.0" prefWidth="114.0" promptText="EMAIL" />
      <TextField fx:id="txtage" layoutX="410.0" layoutY="55.0" prefHeight="25.0" prefWidth="69.0" promptText="AGE" />
      <TableView fx:id="tabla" layoutX="16.0" layoutY="112.0" prefHeight="431.0" prefWidth="573.0" />
      <Label layoutX="160.0" layoutY="12.0" text="Inscripcion A Vacuna">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtapellido" layoutX="160.0" layoutY="55.0" prefHeight="25.0" prefWidth="114.0" promptText="Last Name" />
      <Button fx:id="btncancelar" layoutX="506.0" layoutY="68.0" mnemonicParsing="false" onAction="#cancelar" prefHeight="25.0" prefWidth="69.0" text="Cancelar" visible="false" />
